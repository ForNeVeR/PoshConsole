               <add name="TranscriptListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Transcript.log" />
               <remove name="Default"/>
            </listeners>
         </source>
         <source name="TabExpansion" switchName="TabExpansionSwitch" switchType="System.Diagnostics.SourceSwitch">
            <listeners>
               <add name="PoshCodeLogListener"/>
            </listeners>
         </source>
      </sources>
      <sharedListeners>
         <add name="PoshCodeLogListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="PoshCode.log">
            <!-- filter type="System.Diagnostics.EventTypeFilter" initializeData="Error" -->
         </add>
      </sharedListeners>
